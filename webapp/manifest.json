{
	"_version": "1.12.0",
	"sap.app": {
		"id": "CEMOSA.ZYBC_probando.walkthrough",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZGW_CEMOSA_01_SRV": {
				"uri": "/sap/opu/odata/sap/ZGW_CEMOSA_01_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			},
			"invoiceRemote": {
				"uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "CEMOSA.ZYBC_probando.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "CEMOSA.ZYBC_probando.i18n.i18n"
				}
			},
			"invoice": {
				"dataSource": "invoiceRemote",
				"type": "sap.ui.model.json.JSONModel",
				"uri": "Invoices.json"
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZGW_CEMOSA_01_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "CEMOSA.ZYBC_probando.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
				  "pattern": "",
				  "name": "overview",
				  "target": "overview"
				},
				{
				  "pattern": "detail",
				  "name": "detail",
				  "target": "detail"
				}
	  ],
			"targets": {
				"overview": {
				  "id": "overview",
				  "viewName": "Overview"
				},
				"detail": {
				  "id": "detail",
				  "viewName": "Detail"
				}
			}
		}
	}
}